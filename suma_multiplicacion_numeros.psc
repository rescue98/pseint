Proceso suma_multiplicacion_dosN
	Escribir "Escriba primer numero";
	Leer numero_1;
	Escribir "Escriba segundo numero";
	Leer numero_2;
	suma<-numero_1+numero_2;
	multiplicacion<-numero_1*numero_2;
	Escribir "La suma de los dos numeros es: ",suma;
	Escribir "La multiplicacionde los dos numeros es: ",multiplicacion;
